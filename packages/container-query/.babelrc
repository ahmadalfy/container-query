{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": ["chrome > 60"]
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "@babel/plugin-transform-flow-strip-types",
        [
          "@babel/plugin-transform-runtime",
          {
            "useESModules": true
          }
        ],
        "babel-plugin-lodash"
      ]
    }
  }
}
